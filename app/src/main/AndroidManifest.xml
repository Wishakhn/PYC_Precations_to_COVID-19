<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swankapps.corona.preventcorona">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.PrecautionActivity"></activity>
        <activity android:name=".Activities.Myths_Realities" />
        <activity android:name=".Activities.Remidy_PrecatuionActivity" />
        <activity android:name=".Activities.SymptomsActivity" />
        <activity android:name=".Activities.CovidIntro" />
        <activity android:name=".Activities.SearchActivity" />
        <activity android:name=".Activities.SettingsActivity" />
        <activity android:name=".Activities.profileActivity" />
        <activity android:name=".Activities.AuthOptions" />
        <activity android:name=".Activities.UserAuthentication" />
        <activity android:name=".Activities.IntroSlider" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.MainActivity" />
    </application>

</manifest>